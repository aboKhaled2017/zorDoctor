<!--start our-service-->
<section id='ourServices' class="our-services text-center center-block">
    <h2>@Resource1.ourServices</h2>
    <p class="main-p">@Resource1.ourServicesForYou</p>
    <div class="scroll-btn center-block text-center">
        <i class="fa fa-chevron-up fa-lg"></i>
    </div>
    <div class="container">
        <div class="row center-block">
            <div class="col-md-4 col-sm-12 center-block ">
                <div class="two-left-circles">
                    <div class="circle left-circle-top ">
                        <header>
                            <h3> <img src="~/icon/rays1.png" /> @Resource1.raysCenter</h3>
                        </header>
                        <p>@Resource1.aboutRays</p>
                        <button><i class="fa fa-info"></i> @Resource1.more</button>
                    </div>
                    <div class="circle left-circle-bottom ">
                        <header>
                            <h3> <img src="~/icon/hop1.png" /> @Resource1.hospitals</h3>
                        </header>
                        <p>@Resource1.aboutHospitals</p>
                        <button><i class="fa fa-info"></i> @Resource1.more</button>
                    </div>
                </div>
            </div>
            <div class="col-md-4 col-sm-12 center-block column">
                <div class="circle top-circle">
                    <header>
                        <h3> <img src="~/icon/doc.png" /> @Resource1.doctorReserve</h3>
                    </header>
                    <p>@Resource1.aboutDocReserve</p>
                    <button><i class="fa fa-send"></i> @Resource1.reserveNow</button>
                </div>
                <div class="center-block hidden-sm hidden-xs">
                    <img src="~/images/male-doctor.png" class="img-responsive">
                </div>
                <div class="circle bottom-circle">
                    <header>
                        <h3> <img src="~/icon/servs1.png" /> @Resource1.otherServices</h3>
                    </header>
                    <p>@Resource1.aboutOtherServices</p>
                    <button><i class="fa fa-send"></i> @Resource1.reserveNow</button>
                </div>
            </div>
            <div class="col-md-4 col-sm-12 center-block column">
                <div class="two-left-circles">
                    <div class="circle right-circle-top ">
                        <header>
                            <h3><img src="~/icon/Pharm.png" /> @Resource1.pharmacies</h3>
                        </header>
                        <p>@Resource1.aboutPharmacies</p>
                        <button><i class="fa fa-info"></i> @Resource1.more</button>
                    </div>
                    <div class="circle right-circle-bottom ">
                        <header>
                            <h3> <img src="~/icon/lab1.png" /> @Resource1.labAnalysis</h3>
                        </header>
                        <p>@Resource1.aboutLabAnalysis</p>
                        <button><i class="fa fa-info"></i> @Resource1.more</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!--end our services-->
